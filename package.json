  "files": [
    "dist"
  ],
  "main": "./dist/plugin.umd.js",
  "module": "./dist/plugin.es.js",
  "exports": {
    ".": {
      "import": "./dist/plugin.es.js",
      "require": "./dist/plugin.umd.js"
    }
  },
  "scripts": {
    "dev": "vite serve",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:example": "vite build --config examples/vue3/vite.config.js",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.vue --ignore-pattern node_modules/",
    "lint:style": "./node_modules/stylelint/bin/stylelint.js \"**/**/*.{css,vue}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solomondefi/plugin.git"
  },
  "bugs": {
    "url": "https://github.com/solomondefi/plugin/issues"
  },
  "homepage": "https://github.com/solomondefi/plugin#readme",
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^0.11.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.10.0",
    "postcss": "^8.3.0",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^8.0.0",
    "postcss-modules": "^4.0.0",
    "postcss-nested": "^5.0.5",
    "postcss-simple-vars": "^6.0.3",
