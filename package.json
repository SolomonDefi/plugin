    "stylelint-config-standard": "^22.0.0",
    "vite": "^2.3.5",
    "vite-plugin-components": "^0.10.4",
    "vite-plugin-vue-images": "^0.6.1",
    "vue": "^3.0.11",
    "vue-i18n": "^9.1.6"
  }
